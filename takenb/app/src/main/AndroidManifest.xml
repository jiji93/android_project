<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jihadbensassi.takenb">
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_INTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/takenb"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity"
            android:screenOrientation="portrait">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ScannerViewActivity"
        android:parentActivityName=".MainActivity"
            android:screenOrientation="portrait">
        </activity>
        <activity android:name=".suiviActivity"
            android:screenOrientation="portrait">
        </activity>
        <activity android:name=".HistoriqueActivity"
            android:screenOrientation="portrait">
        </activity>
        <activity android:name=".valider_infoActivity"
            android:screenOrientation="portrait">
        </activity>
        <activity android:name=".Panier"
            android:screenOrientation="portrait">
        </activity>
        <activity android:name=".paiementActivity"
            android:screenOrientation="portrait">
        </activity>
        <activity android:name=".CompteActivity"
            android:screenOrientation="portrait">
        </activity>
        <activity android:name=".PanierActivity"
            android:screenOrientation="portrait">
        </activity>
        <activity android:name=".deleteActivity"
            android:screenOrientation="portrait">
        </activity>
        <activity android:name=".MesenvieActivity"
            android:screenOrientation="portrait">
        </activity>

        <activity android:name=".informationActivity"
            android:screenOrientation="portrait">
        </activity>
    <activity android:name=".Connexion"
        android:screenOrientation="portrait">
    </activity>
        <receiver android:name=".AlarmReceiver"> <intent-filter>
            <action android:name="android.intent.
                        action.ACTION_BATTERY_CHANGED" />
        </intent-filter> </receiver>
    </application>

</manifest>